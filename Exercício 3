#include <iostream>
using namespace std;

int verificarPosNegZero(int numero) {
    if (numero > 0) {
        return 1; 
    } else if (numero < 0) {
        return -1; 
    } else {
        return 0; 
    }
}

int main() {
    int numero;
    
   cin >> numero;

    int resultado = verificarPosNegZero(numero);
    cout << resultado << endl;

    return 0;
}
